nav.navigation
  .navigation__wrapper
    ul.navigation__list
      each item in navigation.data.list
        li.navigation__list__item
          if item.first_link.link_type === 'Web'
            a(href=item.first_link.url rel="noopener" tagert='_blank')=item.first_text
          else 
            a(href=Link(item.first_link))=item.first_text
          if item.second_link.link_type !== 'Any'
            if item.second_link.link_type === 'Web'
              a(href=item.second_link.url rel="noopener" tagert='_blank')=item.second_text
            else 
              a(href=Link(item.second_link))=item.second_text